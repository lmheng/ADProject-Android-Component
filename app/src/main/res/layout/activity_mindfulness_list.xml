<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MindfulnessList"
    android:background="@drawable/bg">
    <TextView
        android:id="@+id/mindfultype"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/curved"
        android:fontFamily="@font/roboto_bold"
        android:text="null"
        android:textColor="#3A3131"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.102" />

    <WebView
        android:id="@+id/mindfullist"
        android:layout_width="match_parent"
        android:layout_height="579dp"
        android:divider="#e5e5ff"
        android:dividerHeight="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mindfultype" />


</androidx.constraintlayout.widget.ConstraintLayout>