<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context=".GetHelp">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".1"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide0_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".25"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".27"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".43" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".45"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".58"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide3_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".6"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".75"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide4_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".77"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guide5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".93"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideV0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".05"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideV1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".95"/>


    <TextView
        android:id="@+id/textView9"
        android:layout_width="172dp"
        android:layout_height="wrap_content"
        android:text="@string/info_5"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toBottomOf="@+id/guide4_1"
        app:layout_constraintBottom_toTopOf="@id/textView10"
        android:translationZ="3dp"/>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button10"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/call"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintTop_toBottomOf="@+id/guide4_1"
        app:layout_constraintBottom_toTopOf="@id/textView10"
        android:translationZ="3dp"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/description_5"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintBottom_toTopOf="@id/guide5"
        android:textSize="12sp"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button9"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/visit"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/button10"
        app:layout_constraintTop_toBottomOf="@+id/guide4_1"
        app:layout_constraintBottom_toTopOf="@id/textView10"
        android:layout_margin="5dp"
        android:translationZ="3dp"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="172dp"
        android:layout_height="wrap_content"
        android:text="@string/info_3"
        app:layout_constraintTop_toTopOf="@+id/guide2_2"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        android:translationZ="3dp"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button8"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/call"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/guide3_1"
        android:translationZ="3dp"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/description_4"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintBottom_toTopOf="@+id/guide4"
        android:textSize="12sp"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button7"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/visit"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/guide3_1"
        app:layout_constraintBottom_toTopOf="@id/textView8"
        android:layout_margin="5dp"
        android:translationZ="3dp"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="172dp"
        android:layout_height="wrap_content"
        android:text="@string/info_4"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toBottomOf="@+id/guide3_1"
        app:layout_constraintBottom_toTopOf="@id/textView8"
        android:translationZ="3dp"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="172dp"
        android:layout_height="wrap_content"
        android:text="@string/info_2"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/guide1_1"
        android:translationZ="3dp"
        />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/description_3"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintBottom_toTopOf="@id/guide3"
        android:textSize="12sp"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/call"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintBottom_toTopOf="@id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/guide2_2"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button5"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/visit"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/guide2_2"
        app:layout_constraintBottom_toTopOf="@id/textView6"
        android:layout_margin="5dp"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button4"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:text="@string/call"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintTop_toBottomOf="@+id/guide1_1"
        android:translationZ="3dp"
        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/description_2"
        app:layout_constraintBottom_toTopOf="@id/guide2"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        android:textSize="12sp"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button3"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/visit"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:layout_margin="5dp"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/guide1_1"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        android:translationZ="3dp"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/guide0"
        />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_medium"
        android:text="Get Help"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="35sp"
        android:translationZ="3dp"
        app:layout_constraintBottom_toTopOf="@+id/guide0"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button1"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/visit"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/guide0_1"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        android:layout_margin="5dp"
        android:translationZ="3dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:text="@string/call"
        android:background="@drawable/rect"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintTop_toTopOf="@+id/guide0_1"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        android:translationZ="3dp"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="138dp"
        android:layout_height="wrap_content"
        android:text="@string/info_1"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintTop_toTopOf="@+id/guide0_1"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        android:translationZ="3dp"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/description_1"
        app:layout_constraintEnd_toEndOf="@id/guideV1"
        app:layout_constraintStart_toStartOf="@id/guideV0"
        app:layout_constraintBottom_toTopOf="@+id/guide1"
        app:layout_constraintTop_toBottomOf="@id/textView1"
        android:textSize="12sp"
        android:translationZ="3dp"/>

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/dialogbox"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/guide0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="2dp"
        android:translationZ = "0dp"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/dialogbox"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="@id/guide0_1"
        app:layout_constraintBottom_toTopOf="@id/guide1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="2dp"
        android:translationZ = "0dp"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/dialogbox"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="@id/guide1_1"
        app:layout_constraintBottom_toTopOf="@id/guide2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="2dp"
        android:translationZ = "0dp"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/dialogbox"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="@id/guide2_2"
        app:layout_constraintBottom_toTopOf="@id/guide3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="2dp"
        android:translationZ = "0dp"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/dialogbox"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="@id/guide3_1"
        app:layout_constraintBottom_toTopOf="@id/guide4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="2dp"
        android:translationZ = "0dp"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/dialogbox"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="@id/guide4_1"
        app:layout_constraintBottom_toTopOf="@id/guide5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:elevation="2dp"
        android:translationZ = "0dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>